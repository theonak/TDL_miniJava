class A {

   public int i;

   public A (int j) {
       this.i = j;
       }

       public void incr() {
           this.i = this.i+1;
       }

}

 public class Main {

   public static void main(String args[]) {

      A a = new A( 2 );

      print( a.i );

      a.i = a.i + 1;

      print( a.i );

      a.incr();

      print( a.i );

   }

   }
